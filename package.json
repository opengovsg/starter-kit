{
  "name": "starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "db:setup": "docker compose -f \"docker-compose.yml\" up -d --wait",
    "db:sleep": "sleep 3",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "setup": "run-s --print-label db:setup db:sleep migrate:dev db:seed",
    "setup:test": "run-s --print-label db:setup db:sleep generate db:seed",
    "teardown": "docker compose down",
    "dev": "next dev",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "build:theme": "npx chakra-cli tokens src/theme/index.ts",
    "start": "next start",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "npm run lint -- --fix",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "dotenv -e .env.test vitest run",
    "test:e2e": "dotenv -e .env.test playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-dev:unit": "dotenv -e .env.test vitest",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "npm run generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.348.0",
    "@aws-sdk/s3-request-presigner": "3.348.0",
    "@chakra-ui/react": "^2.5.1",
    "@hookform/resolvers": "^2.9.11",
    "@opengovsg/design-system-react": "0.1.0-alpha.10",
    "@opengovsg/sgid-client": "^2.0.0",
    "@prisma/client": "^4.15.0",
    "@sendgrid/mail": "^7.7.0",
    "@storybook/jest": "^0.1.0",
    "@tanstack/react-query": "^4.29.1",
    "@tanstack/react-query-devtools": "^4.29.12",
    "@tiptap/extension-link": "^2.0.0-beta.220",
    "@tiptap/extension-placeholder": "^2.0.3",
    "@tiptap/pm": "^2.0.0-beta.220",
    "@tiptap/react": "^2.0.0-beta.220",
    "@tiptap/starter-kit": "^2.0.0-beta.220",
    "@trpc/client": "^10.19.1",
    "@trpc/next": "^10.19.1",
    "@trpc/react-query": "^10.19.1",
    "@trpc/server": "^10.19.1",
    "@types/validator": "^13.7.17",
    "date-fns": "^2.29.3",
    "iron-session": "^6.3.1",
    "jotai": "^2.0.4",
    "lodash": "^4.17.21",
    "next": "^13.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-icons": "^4.8.0",
    "rooks": "^7.11.2",
    "superjson": "^1.12.3",
    "validator": "^13.9.0",
    "winston": "^3.9.0",
    "wretch": "^2.5.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@playwright/test": "^1.33.0",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.18",
    "@storybook/nextjs": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/eslint": "^8.40.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.34",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.56.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.41.0",
    "eslint-config-next": "^13.2.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "msw": "^1.2.1",
