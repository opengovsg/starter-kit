{
  "name": "starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "db:setup": "docker compose -f \"docker-compose.yml\" up -d --wait",
    "db:sleep": "sleep 3",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "setup": "run-s --print-label db:setup db:sleep migrate:dev db:seed",
    "setup:test": "run-s --print-label db:setup db:sleep migrate db:seed",
    "teardown": "docker compose down",
    "dev": "next dev",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "build:theme": "npx chakra-cli tokens src/theme/index.ts",
    "start": "next start",
    "lint": "eslint --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "npm run lint -- --fix",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:unit": "dotenv -e .env.test vitest run",
    "test:e2e": "dotenv -e .env.test playwright test",
    "test-dev": "start-server-and-test dev http://127.0.0.1:3000 test",
    "test-dev:unit": "dotenv -e .env.test vitest",
    "test-start": "start-server-and-test start http://127.0.0.1:3000 test",
    "postinstall": "npm run generate",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.370.0",
    "@aws-sdk/s3-request-presigner": "3.370.0",
    "@chakra-ui/react": "^2.7.1",
    "@hookform/resolvers": "^3.1.1",
    "@opengovsg/design-system-react": "^1.4.0",
    "@opengovsg/sgid-client": "^2.0.0",
    "@paralleldrive/cuid2": "^2.2.1",
    "@prisma/client": "4.15.0",
    "@sendgrid/mail": "^7.7.0",
    "@storybook/jest": "0.2.3",
    "@tanstack/react-query": "^4.29.1",
    "@tanstack/react-query-devtools": "^4.32.0",
    "@tiptap/extension-link": "2.0.3",
    "@tiptap/extension-placeholder": "2.0.3",
    "@tiptap/pm": "2.0.3",
    "@tiptap/react": "2.1.12",
    "@tiptap/starter-kit": "2.1.11",
    "@trpc/client": "^10.31.0",
    "@trpc/next": "^10.31.0",
    "@trpc/react-query": "^10.31.0",
    "@trpc/server": "^10.31.0",
    "@types/validator": "^13.11.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "iron-session": "^6.3.1",
    "jotai": "^2.2.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "next": "^13.5.4",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.2",
    "react-icons": "^4.8.0",
    "superjson": "^1.12.3",
    "trpc-openapi": "^1.2.0",
    "usehooks-ts": "^2.9.1",
    "validator": "^13.11.0",
    "wretch": "^2.7.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@playwright/test": "^1.33.0",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/addon-themes": "7.5.2",
    "@storybook/blocks": "7.5.2",
    "@storybook/nextjs": "7.5.2",
    "@storybook/react": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@types/eslint": "^8.40.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.18",
    "@types/testing-library__jest-dom": "^5.14.6",
    "@typescript-eslint/eslint-plugin": "6.7.2",
