    "typescript": "^4.8.3",
    "vite": "^4.1.5",
    "vitest": "^0.30.1",
    "vitest-environment-vprisma": "^1.2.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "msw": {
    "workerDirectory": "public"
  }
