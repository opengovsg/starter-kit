{
  "name": "starter-kit",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "db:setup": "docker compose -f \"docker-compose.yml\" up -d --wait",
    "db:sleep": "sleep 3",
    "db:seed": "prisma db seed",
    "db:reset": "prisma migrate reset",
    "setup": "run-s --print-label db:setup db:sleep migrate:dev db:seed",
    "setup:test": "run-s --print-label db:setup db:sleep migrate db:seed",
    "teardown": "docker compose down",
    "dev": "next dev",
    "prebuild": "run-s generate migrate",
    "build": "next build",
    "build:theme": "npx chakra-cli tokens src/theme/index.ts",
    "start": "next start",
    "lint": "eslint --report-unused-disable-directives --report-unused-disable-directives .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .prettierignore --config .prettierrc.cjs",
    "format:fix": "prettier --write \"**/*.{js,cjs,mjs,ts,tsx,md,json}\" --ignore-path .prettierignore --config .prettierrc.cjs",
    "migrate:dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "test": "run-s test:*",
    "test:setup": "docker compose -f \"docker-compose.test.yml\" up -d --wait",
    "test:vitest": "dotenv -e .env.test vitest run",
    "test-dev:vitest": "dotenv -e .env.test vitest",
    "test:e2e": "dotenv -e .env.test playwright test",
    "test-ci:coverage": "dotenv -e .env.test vitest run -- --coverage",
    "test-ci:e2e": "start-server-and-test start http://127.0.0.1:3000 test:e2e",
    "test-dev:e2e": "start-server-and-test dev http://127.0.0.1:3000 test:e2e",
    "load-test:build": "webpack --config tests/load/webpack.config.js",
    "postinstall": "npm run generate && npm run build:theme",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.774.0",
    "@aws-sdk/s3-request-presigner": "3.774.0",
    "@chakra-ui/anatomy": "^2.3.4",
    "@chakra-ui/react": "^2.8.2",
    "@chakra-ui/styled-system": "^2.12.0",
    "@chakra-ui/theme-tools": "^2.1.2",
    "@chakra-ui/utils": "^2.0.15",
    "@hookform/resolvers": "^3.3.4",
    "@opengovsg/design-system-react": "^1.24.0",
    "@opengovsg/sgid-client": "^2.3.0",
    "@opengovsg/starter-kitty-validators": "^1.2.11",
    "@prisma/client": "^5.7.1",
    "@sendgrid/mail": "^8.1.3",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-query-devtools": "^4.36.1",
    "@trpc/client": "10.45.2",
    "@trpc/next": "10.45.2",
    "@trpc/react-query": "10.45.2",
    "@trpc/server": "10.45.2",
    "@types/lodash": "^4.14.202",
    "@types/validator": "^13.11.7",
    "iron-session": "^8.0.4",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.8",
    "next": "15.5.5",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^5.0.0",
    "react-hook-form": "^7.51.4",
    "react-icons": "^4.12.0",
    "superjson": "^2.2.1",
    "type-fest": "^4.26.1",
    "usehooks-ts": "^2.9.2",
    "validator": "^13.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-properties": "^7.23.3",
    "@babel/plugin-transform-object-rest-spread": "^7.23.4",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@chakra-ui/cli": "^2.4.1",
    "@date-fns/tz": "^1.2.0",
    "@devoxa/integresql-client": "^2.1.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@playwright/test": "^1.40.1",
    "@storybook/addon-a11y": "8.4.7",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
