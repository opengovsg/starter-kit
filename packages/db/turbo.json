{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tasks": {
    "generate": {
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["generate"]
    },
    "lint": {
      "dependsOn": ["generate"]
    },
    "build": {
      "dependsOn": ["generate"]
    },
    "dev": {
      "dependsOn": ["generate"]
    }
  }
}
